{
  "obj": [
    {
      "ctrls": {
        "in_names": [],
        "in_size": -1,
        "out_names": ["image", "text"],
        "out_size": 2
      },
      "info": {
        "author": "zer0",
        "backend": "builtin",
        "bugs": "https://answer.bogonets.com/",
        "dependencies": [],
        "descriptions": {
          "en": "Displays the current time as an image.",
          "ko": "\ud604\uc7ac \uc2dc\uac04\uc744 \uc774\ubbf8\uc9c0\ub85c \ucd9c\ub825\ud55c\ub2e4."
        },
        "documentation_mime": "text",
        "documentations": {},
        "engines": "1.0.2",
        "homepage": "https://answer.bogonets.com/",
        "keywords": ["builtin"],
        "license": "Bogonet License",
        "meta": {
          "gui": {
            "name": "roi_setting"
          },
          "components": {
            "top": [
              {
                "align": "left",
                "name": "signal-button",
                "ref": "signalButton",
                "transData": "image.setSource",
                "props": {
                  "small": false,
                  "color": "blue",
                  "name": "signal",
                  "propertyName": "shapes",
                  "signal": {
                    "name": "~${OWN}.preview",
                    "input_queries": [],
                    "output_queries": ["${OWN}.preview?mime=image/jpeg"]
                  }
                },
                "style": ""
              },
              {
                "align": "right",
                "name": "get-property-button",
                "ref": "getPropertyButton",
                "transData": "canvas.setRectangles",
                "props": {
                  "small": true,
                  "color": "teal",
                  "name": "get-property",
                  "propertyName": "shapes"
                },
                "style": ""
              },
              {
                "align": "right",
                "name": "set-property-button",
                "ref": "setPropertyButton",
                "sendData": "canvas.getRectangles",
                "props": {
                  "small": true,
                  "color": "teal",
                  "name": "set-property",
                  "propertyName": "shapes"
                },
                "style": ""
              }
            ],
            "body": [
              {
                "name": "viewImage",
                "ref": "image",
                "props": {},
                "style": ""
              },
              {
                "name": "drawCanvas",
                "ref": "canvas",
                "props": {
                  "useShapes": ["rectangle"],
                  "limitShape": 1
                },
                "style": ""
              }
            ]
          }
        },
        "name": "dynamic_clock_image",
        "titles": {
          "en": "Clock Image",
          "ko": "테스트 람다"
        },
        "use_system_packages": true,
        "version": "1.0.2"
      },
      "props": [
        {
          "default_value": "20.0",
          "help": {
            "en": "The size of the font to display the time.",
            "ko": "\uc2dc\uac04\uc744 \ud45c\uc2dc\ud560 \ud3f0\ud2b8\uc758 \ud06c\uae30."
          },
          "name": "font_size",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Font size",
            "ko": "\ud3f0\ud2b8 \ud06c\uae30"
          },
          "type": "float",
          "valid": {}
        },
        {
          "default_value": "%Y-%m-%dT%H:%M:%S",
          "help": {
            "en": "How to print the current time.",
            "ko": "\ud604\uc7ac \uc2dc\uac04\uc744 \ucd9c\ub825\ud560 \ubc29\ubc95."
          },
          "name": "time_format",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Time format",
            "ko": "\uc2dc\uac04 \ud3ec\ub9f7"
          },
          "type": "str",
          "valid": {}
        },
        {
          "default_value": "3",
          "help": {
            "en": "The channels of the image to be printed.",
            "ko": "\ucd9c\ub825\ud560 \uc774\ubbf8\uc9c0\uc758 \ucc44\ub110."
          },
          "name": "channels",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Image channels",
            "ko": "\uc774\ubbf8\uc9c0 \ucc44\ub110"
          },
          "type": "int",
          "valid": {
            "list": "1;3;4"
          }
        },
        {
          "default_value": "#FFFFFFFF",
          "help": {
            "en": "The color of the text to display the time.",
            "ko": "\uc2dc\uac04\uc744 \ud45c\uc2dc\ud560 \ubb38\uc790 \uc0c9\uc0c1."
          },
          "name": "fg_color",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Text color",
            "ko": "\ubb38\uc790 \uc0c9\uc0c1"
          },
          "type": "color",
          "valid": {}
        },
        {
          "default_value": "480",
          "help": {
            "en": "The height of the image to be printed.",
            "ko": "\ucd9c\ub825\ud560 \uc774\ubbf8\uc9c0\uc758 \ub192\uc774."
          },
          "name": "height",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Image height",
            "ko": "\uc774\ubbf8\uc9c0 \ub192\uc774"
          },
          "type": "int",
          "valid": {}
        },
        {
          "default_value": "Time: ",
          "help": {
            "en": "The prefix text to be printed statically.",
            "ko": "\uace0\uc815\uc801\uc73c\ub85c \ucd9c\ub825\ub420 \uc811\ub450\uc0ac \ubb38\uc790\uc5f4."
          },
          "name": "prefix_text",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Prefix text",
            "ko": "\uc811\ub450\uc0ac"
          },
          "type": "str",
          "valid": {}
        },
        {
          "default_value": "#000000FF",
          "help": {
            "en": "Background color of the image.",
            "ko": "\uc774\ubbf8\uc9c0\uc758 \ubc30\uacbd \uc0c9\uc0c1."
          },
          "name": "bg_color",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Background color",
            "ko": "\ubc30\uacbd \uc0c9\uc0c1"
          },
          "type": "color",
          "valid": {}
        },
        {
          "default_value": "480",
          "help": {
            "en": "The width of the image to be printed.",
            "ko": "\ucd9c\ub825\ud560 \uc774\ubbf8\uc9c0\uc758 \ub108\ube44."
          },
          "name": "width",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "Image width",
            "ko": "\uc774\ubbf8\uc9c0 \ub108\ube44"
          },
          "type": "int",
          "valid": {}
        },
        {
          "default_value": "0.5,0.5,0.2,0.2",
          "help": {
            "en": "How to print the current time.",
            "ko": "\ud604\uc7ac \uc2dc\uac04\uc744 \ucd9c\ub825\ud560 \ubc29\ubc95."
          },
          "name": "roi_setting",
          "required": true,
          "rule": "read_and_write",
          "title": {
            "en": "ROI setting",
            "ko": "ROI 설정"
          },
          "type": "csv",
          "valid": {}
        }
      ]
    }
  ]
}
