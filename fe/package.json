{
  "name": "@recc/fe",
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --no-clean --dest ../core/recc/www",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "@recc/api": "^2.0.0-dev10",
    "@toast-ui/vue-grid": "^4.21.0",
    "chart.js": "^3.5.1",
    "chartjs-adapter-moment": "^1.0.0",
    "chartjs-plugin-streaming": "^2.0.0",
    "core-js": "^3.22.2",
    "downloadjs": "^1.4.7",
    "dragula": "^3.7.3",
    "fontfaceobserver": "^2.1.0",
    "hls.js": "^1.1.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "pixi-text-input": "^1.0.6",
    "pixi.js": "^6.3.0",
    "register-service-worker": "^1.7.1",
    "roboto-fontface": "^0.10.0",
    "sha256": "^0.2.0",
    "viewerjs": "^1.10.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.22.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue-toastification": "^1.7.14",
    "vuetify": "^2.5.8",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@mdi/font": "^6.6.96",
    "@types/dragula": "^3.7.1",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.172",
    "@types/webrtc": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-e2e-cypress": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-pwa": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-plugin-unit-jest": "~5.0.4",
    "@vue/cli-plugin-vuex": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^1.2.2",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "cypress": "^8.3.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.1.0",
    "node-sass": "^7.0.1",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "prettier": "^2.6.2",
    "sass": "1.32.8",
    "sass-loader": "10",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "webextensions": true
    },
    "globals": {
      "RTCIceGatheringState": true,
      "RTCIceServer": true,
      "RTCConfiguration": true,
      "RTCSessionDescriptionInit": true,
      "RTCErrorEvent": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "prettier/prettier": "error",
      "vue/valid-v-slot": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {
    "printWidth": 88,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": false,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "ignore",
    "vueIndentScriptAndStyle": false,
    "endOfLine": "lf"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "setupFiles": [
      "./tests/unit/setup.ts"
    ]
  }
}
